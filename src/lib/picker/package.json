{
  "$schema": "../../../node_modules/ng-packagr/package.schema.json",
  "name": "@coyo/ngx-emoji-mart",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://mindsmash.jfrog.io/mindsmash/api/npm/coyo-frontend/"
  },
  "description": "Customizable Slack-like emoji picker for Angular",
  "peerDependencies": {
    "@angular/core": "^6.0.0 || ^7.0.0",
    "@angular/common": "^6.0.0 || ^7.0.0"
  },
  "bugs": "https://github.com/TypeCtrl/ngx-emoji-mart/issues",
  "homepage": "https://github.com/TypeCtrl/ngx-emoji-mart",
  "repository": "mindsmash/ngx-emoji-mart",
  "license": "MIT",
  "sideEffects": [ "*.css" ],
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts",
      "languageLevel": ["dom", "es2017"],
      "umdModuleIds": {
        "@coyo/ngx-emoji-mart/ngx-emoji": "@coyo/ngx-emoji-mart/ngx-emoji"
      }
    },
    "dest": "../../../dist/picker"
  },
  "keywords": [
    "angular",
    "emoji",
    "picker"
  ]
}
